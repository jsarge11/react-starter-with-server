{
	"name": "react-starter",
	"version": "1.0.0",
	"description": "boilerplate for non CRA build",
	"main": "server/app.ts",
	"scripts": {
		"start": "webpack-dev-server --mode=development --hot",
		"types": "tsc --noEmit",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"build": "npm run build:fe && npm run build:be",
		"build:fe": "webpack --mode=production --progress",
		"build:be": "tsc --project server/tsconfig.json"
	},
	"keywords": [
		"react",
		"start",
		"boilerplate",
		"webpack",
		"babel"
	],
	"author": "Jay Sargent",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@babel/preset-react": "^7.22.5",
		"@types/axios": "^0.14.0",
		"@types/node": "^20.4.5",
		"@types/react": "^18.2.17",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"babel-loader": "^9.1.3",
		"css-loader": "^6.8.1",
		"dotenv-webpack": "^8.0.1",
		"eslint": "^8.45.0",
		"eslint-plugin-react": "^7.33.0",
		"html-webpack-plugin": "^5.5.3",
		"husky": "^8.0.3",
		"prettier": "^3.0.0",
		"sass": "^1.64.1",
		"sass-loader": "^13.3.2",
		"style-loader": "^3.3.3",
		"ts-loader": "^9.4.4",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"dependencies": {
		"@types/express": "^4.17.17",
		"axios": "^1.4.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"nodemonConfig": {
		"ignore": [
			"src"
		]
	}
}
